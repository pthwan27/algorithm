package com.swea.algorithm;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		int N = 0;
		int M = 0;

		int[][] area;

		for (int tc = 1; tc <= T; tc++) {
			N = sc.nextInt();
			M = sc.nextInt();

			area = new int[N][M];

			for (int i = 0; i < N; i++) {
				for (int j = 0; j < M; j++) {
					area[i][j] = sc.nextInt();
				}
			}
			// 1등한 사람 수 / 푼 문제 수
			int personCount = 0;
			int problemCount = 0;

			int maxProCount = -1;

			for (int i = 0; i < N; i++) {
				for (int j = 0; j < M; j++) {
					if (area[i][j] == 1) {
						problemCount++;
					}
				}
				if (maxProCount == problemCount) {
					personCount++;

					maxProCount = problemCount;
				} else if (problemCount > maxProCount) {					
					personCount = 1;

					maxProCount = problemCount;
				} else {

				}

			}

			System.out.printf("#%d %d %d", tc, personCount, problemCount);
		}
	}

}
