package com.swea.algorithm;

import java.util.Arrays;
import java.util.Scanner;

class Solution {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int MAX_VALUE = Integer.MIN_VALUE;
		int MIN_VALUE = Integer.MAX_VALUE;
		
		int T = sc.nextInt();
		
		
		//한번만 바꾸는 거라면, 자릿수가 제일 중요!		
		for(int tc = 1; tc <= T; tc++) {
			int N = sc.nextInt();
			
			MAX_VALUE = N;
			MIN_VALUE = N;
			
			String numStr = String.valueOf(N);
			
			char[] numChar = new char[numStr.length()];				
			char[] originNumChar = new char[numStr.length()];	
			
			for(int i = 0; i < numStr.length(); i++) {
				numChar[i] = numStr.charAt(i);
				originNumChar[i] = numStr.charAt(i); 
			}
			
			for(int i = 0; i < numStr.length(); i++) {
				for(int j = numStr.length()-1; j > 0; j--){
					if(i == j) {
						continue;
					}
					
					//숫자 한쌍 위치 바꾸기
					char c1 = numChar[i];
					numChar[i] = numChar[j];
					numChar[j] = c1;
					
					//첫번째 숫자가 0이면 안됌
					if(numChar[0] == '0') {
						continue;
					}
					else
					{
						String changeNumStr = new String(numChar);
						int changeNum = Integer.parseInt(changeNumStr);
						
						if(changeNum > MAX_VALUE) {
							MAX_VALUE = changeNum;
						}
						
						if(changeNum < MIN_VALUE) {
							MIN_VALUE = changeNum;							
						}
						
					}
					numChar = Arrays.copyOf(originNumChar, numChar.length);
				}
			}			
			System.out.printf("#%d %d %d%n", tc, MIN_VALUE, MAX_VALUE);
		}
	}
}
